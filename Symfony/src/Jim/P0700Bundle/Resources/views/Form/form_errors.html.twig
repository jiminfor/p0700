{% block form_errors %}
{% if errors|length > 0 %}
<div class="errors">
    {% for error in errors %}
    	<div class="error">{{ error.messagePluralization is null ? error.messageTemplate|trans(error.messageParameters, 'validators') : error.messageTemplate|transchoice(error.messagePluralization, error.messageParameters, 'validators') }}</div>
	{% endfor %}
{% endif %}
{% endblock form_errors %}
